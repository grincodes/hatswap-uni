#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/grincodes/Documents/Begrudges/BlockChain/Solidity/Defi/Sushiswap/sushiswap/node_modules/.pnpm/nanoid@4.0.0/node_modules/nanoid/bin/node_modules:/Users/grincodes/Documents/Begrudges/BlockChain/Solidity/Defi/Sushiswap/sushiswap/node_modules/.pnpm/nanoid@4.0.0/node_modules/nanoid/node_modules:/Users/grincodes/Documents/Begrudges/BlockChain/Solidity/Defi/Sushiswap/sushiswap/node_modules/.pnpm/nanoid@4.0.0/node_modules:/Users/grincodes/Documents/Begrudges/BlockChain/Solidity/Defi/Sushiswap/sushiswap/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/grincodes/Documents/Begrudges/BlockChain/Solidity/Defi/Sushiswap/sushiswap/node_modules/.pnpm/nanoid@4.0.0/node_modules/nanoid/bin/node_modules:/Users/grincodes/Documents/Begrudges/BlockChain/Solidity/Defi/Sushiswap/sushiswap/node_modules/.pnpm/nanoid@4.0.0/node_modules/nanoid/node_modules:/Users/grincodes/Documents/Begrudges/BlockChain/Solidity/Defi/Sushiswap/sushiswap/node_modules/.pnpm/nanoid@4.0.0/node_modules:/Users/grincodes/Documents/Begrudges/BlockChain/Solidity/Defi/Sushiswap/sushiswap/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nanoid/bin/nanoid.js" "$@"
else
  exec node  "$basedir/../nanoid/bin/nanoid.js" "$@"
fi
